;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns.gnet.net.id. root.gnet.net.id. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	gnet.net.id.
252	IN	PTR	gnet.net.id.
252	IN	PTR	www.gnet.net.id.
